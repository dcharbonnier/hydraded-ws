{
  "name": "advanced-websocket",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "description": "The toolbox for websockets clients, reconnecting websockets, channels muxing websockets, authentication, json-rpc over websockets",
  "keywords": [
    "websockets",
    "websocket",
    "json-rpc",
    "json-rpc2",
    "rpc",
    "reconnecting",
    "reconnection",
    "reconnect",
    "muxing",
    "muxer",
    "demuxer",
    "authentication"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dcharbonnier/advanced-websocket.git"
  },
  "bugs": {
    "url": "http://github.com/dcharbonnier/advanced-websocket/issues"
  },
  "scripts": {
    "test": "yarn run test-node && yarn run test-karma",
    "test-server": "node --experimental-modules mock.mjs >/dev/null 2>&1 &",
    "test-karma": "yarn run test-server && karma start --single-run",
    "test-phantom": "yarn run test-server && karma start --single-run --browsers PhantomJS",
    "test-chrome": "yarn run test-server && karma start --single-run --browsers Chrome",
    "test-node": "yarn run test-server && mocha --opts mocha.opts",
    "lint": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "codecov": "codecov",
    "prepublishOnly": "tsc -p ./tsconfig.json --outDir dist/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "David Charbonnier",
  "dependencies": null,
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/es6-collections": "^0.5.31",
    "@types/es6-promise": "3.3.0",
    "@types/karma": "^1.7.2",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.55",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "es6-promise": "^4.1.1",
    "forever": "^0.15.3",
    "karma": "^2.0.0",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-helpful-reporter": "^0.3.4",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-promise": "^0.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-typescript": "^3.0.8",
    "karma-typescript-es6-transform": "^1.0.2",
    "mocha": "^4.0.1",
    "mock-socket": "^7.1.0",
    "puppeteer": "^0.13.0",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "ws": "^4.0.0"
  }
}
